<p id="notice"><%= notice %></p>
<br>
<div class="col-md-12">
  <div class="btn-group btn-group-justified">
    <%= link_to 'Nueva Calificacion', new_calificacion_path, class: 'btn btn-default'  %>
  
    <%= link_to 'Volver', root_path, class: 'btn btn-default' %>

  </div>
</div>
<br><br>
<h1>Califique sus compras</h1>
<div class="row">
  <div class="col-md-12">
    <div class="listing">
      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Compra</th>
              <th>Valor</th>
              <th colspan="3"></th>
            </tr>
            <% if flash[:error] %>
              <div class="alert alert-danger"><%= flash[:error] %></div>
            <% end %>
            </br>
          </thead>

          <tbody>
            <%= render @calificacions %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>


<br>