<div class="section-container--header row">
  <div class="col-md-10 col-xs-8">
    <h1>Exportar consumos para GNS</h1> 
    
  </div>
</div>

<% if flash[:error] %>
    <div class="alert alert-danger"><%= flash[:error] %></div>
  <% end %>
</br>
<div class="panel-body form-group">
<%= form_for(:consultaConsumos, url:{:controller => 'gns', :action => 'consumos'}) do |f| %>
      <div class="panel-body form-group  ">       
   		 <p><%= f.label "Fecha inicial" %></p>
   			 <%= f.date_select :fechaIni, class: 'form-control' %>
   			 </br>
   			 </br>
  		 <p><%= f.label "Fecha Final" %></p>
   			 <%= f.date_select :fechaFin, class: 'form-control' %>
    	 <p><%= f.label "Empresa" %></p>
          <%= f.select :empresa_id,  Empresa.all.collect {|e| [ e.nombre, e.id ] }, { include_blank: 'Seleccionar empresa' }, class: 'form-control' %>
 		</div>
        <div class="col-md-12">
      <div class="btn-group btn-group-justified">
        
        <div class="btn-group">
        <%= f.button "Generar archivo", class: 'btn btn-info prueboHabilitar ' %>
        </div>
        <div class="btn-group">
        <%= link_to 'Volver', { :controller => :bi, :action => :index }, class: 'btn btn-info '  %>
        </div>
      </div>
    </div>
  </div>
  </div>
  <br>

<% end %>

 

 