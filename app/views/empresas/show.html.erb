<p id="notice"><%= notice %></p>
<p>
  <strong>RUT:</strong>
  <%= @empresa.rut %>
</p>
<p>
  <strong>Nombre:</strong>
  <%= @empresa.nombre %>
</p>

<p>
  <strong>Razon Social:</strong>
  <%= @empresa.razon_social %>
</p>

<p>
  <strong>Telefono:</strong>
  <%= @empresa.telefono %>
</p>

<p>
  <strong>Direccion:</strong>
  <%= @empresa.direccion %>
</p>
<p>
  <strong>Correo electronico:</strong>
  <%= @empresa.email %>
</p>
<div class="btn-group-justified">
<%= link_to '', edit_empresa_path(@empresa), class: 'btn btn-default btn-sm glyphicon glyphicon-pencil', :title => "Editar" %>
    
    
    <%= link_to '',
                @empresa,
                method: :delete,
                class: 'btn btn-warning btn-sm glyphicon glyphicon-remove',
                data: { confirm: 'Esta seguro de eliminar esta empresa?' }, :title => "Eliminar" %>

</div>
</br>
</br>
<div class="btn-group-justified">
  <%= link_to 'Volver', empresas_path, class: 'btn btn-info' %>
</div>
